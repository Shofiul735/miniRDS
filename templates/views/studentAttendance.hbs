{{>header}}
<section>
    <table class="table table-dark">
        <thead>
            <tr class="text-center">
                <th scope="col">Date</th>
                <th>Present</th>
             </tr>
     </thead>
    <tbody>
        {{#each data}}
            {{#if this.present}}
                <tr class="bg-success text-center">
                    <td>{{this.date}}</td>
                    <td>Yes</td>
                </tr>
            {{else}}
                <tr class="bg-danger text-center">
                    <td>{{this.date}}</td>
                    <td>No</td>
                 </tr>
            {{/if}}
        {{/each}}
</tbody>
</table>
</section>

{{>footer}}